{
  "name": "markdownlint-config-travi",
  "description": "my shareable config for markdownlint",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/markdownlint-config-travi",
  "bugs": "https://github.com/travi/markdownlint-config-travi/issues",
  "homepage": "https://github.com/travi/markdownlint-config-travi#readme",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "test": "run-s lint:*",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "cz-conventional-changelog": "2.1.0",
    "globstar": "1.0.0",
    "husky": "0.14.3",
    "markdownlint-cli": "0.5.0",
    "npm-run-all": "4.1.2",
    "validate-commit-msg": "2.14.0"
  }
}
